<% Category.all.each do |category| %>
  <li>
      <%= link_to category.name, category_path(category) %>
  </li> 
<% end %>
<% if logged_in? and current_user.admin? %>
    <li role="separator" class="divider"></li>
    <li>
        <%= link_to 'Create New Category', new_category_path %>
    </li>
<% end %>